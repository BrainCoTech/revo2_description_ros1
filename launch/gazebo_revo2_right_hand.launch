<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
  <node
    name="tf_footprint_base_revo2_right_hand"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 0 0 0 0 base_link base_footprint 40" />
  <node
    name="spawn_model_revo2_right_hand"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find revo2_description)/urdf/revo2_right_hand.urdf -urdf -model revo2_right_hand"
    output="screen" />
  <node
    name="fake_joint_calibration_revo2_right_hand"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" />
</launch>
